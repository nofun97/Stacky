{
  "name": "stacky",
  "version": "1.0.0",
  "description": "A skill sharing platform",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node ./server/server.js",
    "server": "nodemon ./server/server.js",
    "client": "npm start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "npm run heroku-postbuild && npm run start",
    "test": "NODE_ENV=test mocha ./server/test --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nofun97/Stacky.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nofun97/Stacky/issues"
  },
  "homepage": "https://github.com/nofun97/Stacky#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.1",
    "formik": "^1.5.4",
    "jsonwebtoken": "^8.5.1",
    "memorystore": "^1.6.1",
    "mocha": "^6.1.4",
    "mongoose": "^5.7.5",
    "mongoose-paginate-v2": "^1.2.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "sinon": "^7.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "on-headers": "^1.0.2",
    "supertest": "^4.0.2"
  }
}
